generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("PSQL_URL")
}

model User {
    id Int @id @default(autoincrement())

    fullName  String
    email     String @unique
    password  String
    avatarUrl String

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
